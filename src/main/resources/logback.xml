<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<configuration debug="false">

  <appender name="DB" class="com.ossys.ae.logging.AEDatabaseAppender">
  </appender>

  <appender name="CONSOLE" class="com.ossys.ae.logging.AEConsoleAppender">
    <encoder>
      <pattern>%d{HH:mm:ss.SSS} [%thread] %-5level %logger{36} - %msg%n</pattern>
    </encoder>
  </appender>

  <root level="INFO">
    <appender-ref ref="DB" />
    <appender-ref ref="CONSOLE" />
  </root>
</configuration>